      "Unique Products Viewed: 166,794\n",
      "Memory after: 200.34 MB\n",
      "\n",
      "Execution time: 113.84 seconds (1.90 minutes)\n",
      "Memory used: 3.04 MB (0.00 GB)\n"
     ]
    }
   ],
   "source": [
    "def method1_pandas_chunks_analysis():\n",
    "    \"\"\"\n",
    "    Method 1 Enhanced: Read CSV in chunks and perform aggregation analysis\n",
    "    This demonstrates how to analyze data without loading entire file into memory\n",
    "    \"\"\"\n",
    "    print(\"\\nPerforming aggregation analysis using chunks...\")\n",
    "    \n",
    "    chunk_size = 500000\n",
    "    \n",
    "    # Variables to store aggregated results\n",
    "    event_type_counts = {}\n",
    "    total_revenue = 0\n",
    "    product_views = set()\n",
    "    \n",
    "    # Read and process each chunk\n",
    "    for i, chunk in enumerate(pd.read_csv(file_path, chunksize=chunk_size), 1):\n",
    "        \n",
    "        # Count event types\n",
    "        event_counts = chunk['event_type'].value_counts()\n",
    "        for event, count in event_counts.items():\n",
    "            event_type_counts[event] = event_type_counts.get(event, 0) + count\n",
    "        \n",
    "        # Calculate revenue from purchases\n",
    "        purchases = chunk[chunk['event_type'] == 'purchase']\n",
    "        total_revenue += purchases['price'].sum()\n",
    "        \n",
    "        # Track unique product views\n",
    "        views = chunk[chunk['event_type'] == 'view']['product_id'].unique()\n",
    "        product_views.update(views)\n",
    "        \n",
    "        if i % 5 == 0:\n",
    "            print(f\"  Processed {i} chunks...\")\n",
    "    \n",
    "    # Create results dictionary\n",
    "    results = {\n",
    "        'event_type_distribution': pd.Series(event_type_counts),\n",
    "        'total_revenue': total_revenue,\n",
    "        'unique_products_viewed': len(product_views)\n",
    "    }\n",
    "    \n",
    "    print(f\"\\nEvent Type Distribution:\")\n",
    "    print(results['event_type_distribution'])\n",
    "    print(f\"\\nTotal Revenue: ${total_revenue:,.2f}\")\n",
    "    print(f\"Unique Products Viewed: {results['unique_products_viewed']:,}\")\n",
    "    \n",
    "    return results\n",
    "\n",
    "\n",
    "# Execute Method 1 Enhanced\n",
    "result_method1_analysis = measure_performance(\n",
    "    method1_pandas_chunks_analysis, \n",
    "    \"Method 1 Enhanced: Pandas Chunks with Analysis\"\n",
    ")\n"
   ]
  },
