   ],
   "source": [
    "def method2_dask_dataframe():\n",
    "    \"\"\"\n",
    "    Method 2: Read large CSV file using Dask\n",
    "    Dask automatically partitions data and uses lazy evaluation\n",
    "    \"\"\"\n",
    "    print(\"\\nReading file using Dask...\")\n",
    "    \n",
    "    # Read CSV using Dask\n",
    "    # Dask automatically partitions the data into chunks\n",
    "    dask_df = dd.read_csv(\n",
    "        file_path,\n",
    "        parse_dates=['event_time'],\n",
    "        blocksize='256MB'  # Size of each partition (adjust as needed)\n",
    "    )\n",
    "    \n",
    "    print(f\"Dask DataFrame created\")\n",
    "    print(f\"Number of partitions: {dask_df.npartitions}\")\n",
    "    print(f\"Columns: {list(dask_df.columns)}\")\n",
    "    \n",
    "    # Get basic information (lazy evaluation - doesn't compute yet)\n",
    "    print(\"\\nDataFrame Info:\")\n",
    "    print(f\"Total rows (estimated): {len(dask_df):,}\")\n",
    "    \n",
    "    # Perform some computations (this triggers actual computation)\n",
    "    print(\"\\nComputing statistics...\")\n",
    "    \n",
    "    # Count rows (this executes the computation)\n",
    "    total_rows = len(dask_df)\n",
    "    \n",
    "    # Get event type distribution\n",
    "    event_distribution = dask_df['event_type'].value_counts().compute()\n",
    "    \n",
    "    # Calculate total revenue\n",
    "    total_revenue = dask_df[dask_df['event_type'] == 'purchase']['price'].sum().compute()\n",
    "    \n",
    "    # Count unique users\n",
    "    unique_users = dask_df['user_id'].nunique().compute()\n",
    "    \n",
    "    print(f\"\\nTotal rows: {total_rows:,}\")\n",
    "    print(f\"\\nEvent Type Distribution:\")\n",
    "    print(event_distribution)\n",
    "    print(f\"\\nTotal Revenue: ${total_revenue:,.2f}\")\n",
    "    print(f\"Unique Users: {unique_users:,}\")\n",
    "    \n",
    "    return {\n",
    "        'dask_df': dask_df,\n",
    "        'total_rows': total_rows,\n",
    "        'event_distribution': event_distribution,\n",
    "        'total_revenue': total_revenue,\n",
    "        'unique_users': unique_users\n",
    "    }\n",
    "\n",
    "\n",
    "# Execute Method 2\n",
    "result_method2 = measure_performance(method2_dask_dataframe, \"Method 2: Dask DataFrame\")\n"
   ]
  },
