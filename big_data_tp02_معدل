    "# 7: Method 3 - Using Compression (Save as Parquet)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "bca3e725-b8c5-4cfc-884c-d746ab453b12",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "======================================================================\n",
      "Testing Method: Method 3: Compression (Parquet)\n",
      "======================================================================\n",
      "Memory before: 322.52 MB\n",
      "\n",
      "Converting CSV to Parquet format...\n",
      "Writing to Parquet format...\n",
      "Parquet file created at: C:\\Users\\Rade\\Desktop\\TP\\BData\\2019-Oct.parquet\n",
      "\n",
      "File Size Comparison:\n",
      "Original CSV: 5.28 GB\n",
      "Compressed Parquet: 1.50 GB\n",
      "Compression Ratio: 71.50%\n",
      "Space Saved: 3.77 GB\n",
      "\n",
      "Reading from Parquet format...\n",
      "Parquet read time: 0.42 seconds\n",
      "Total rows: 42,448,764\n",
      "Memory after: 356.60 MB\n",
      "\n",
      "Execution time: 233.42 seconds (3.89 minutes)\n",
      "Memory used: 34.08 MB (0.03 GB)\n"
     ]
    }
   ],
   "source": [
    "def method3_compression():\n",
    "    \"\"\"\n",
    "    Method 3: Convert CSV to compressed Parquet format\n",
    "    Parquet is a columnar storage format that is much more efficient\n",
    "    \"\"\"\n",
    "    print(\"\\nConverting CSV to Parquet format...\")\n",
    "    \n",
    "    # Output file path\n",
    "    parquet_path = r'C:\\Users\\Rade\\Desktop\\TP\\BData\\2019-Oct.parquet'\n",
    "    \n",
    "    # Use Dask to read CSV and save as Parquet\n",
    "    dask_df = dd.read_csv(\n",
    "        file_path,\n",
    "        parse_dates=['event_time'],\n",
    "        blocksize='256MB'\n",
    "    )\n",
    "    \n",
    "    print(\"Writing to Parquet format...\")\n",
    "    dask_df.to_parquet(\n",
    "        parquet_path,\n",
    "        engine='pyarrow',\n",
    "        compression='snappy'  # Use snappy compression algorithm\n",
    "    )\n",
    "    \n",
    "    print(f\"Parquet file created at: {parquet_path}\")\n",
    "    \n",
    "    # Get file sizes for comparison\n",
    "    csv_size = os.path.getsize(file_path) / (1024**3)  # GB\n",
    "    \n",
    "    # Parquet creates directory with multiple files, calculate total size\n",
    "    parquet_size = 0\n",
    "    if os.path.isdir(parquet_path):\n",
    "        for file in os.listdir(parquet_path):\n",
    "            file_full_path = os.path.join(parquet_path, file)\n",
    "            if os.path.isfile(file_full_path):\n",
    "                parquet_size += os.path.getsize(file_full_path)\n",
    "    \n",
    "    parquet_size_gb = parquet_size / (1024**3)\n",
    "    \n",
    "    compression_ratio = (1 - parquet_size_gb / csv_size) * 100\n",
    "    \n",
    "    print(f\"\\nFile Size Comparison:\")\n",
    "    print(f\"Original CSV: {csv_size:.2f} GB\")\n",
    "    print(f\"Compressed Parquet: {parquet_size_gb:.2f} GB\")\n",
    "    print(f\"Compression Ratio: {compression_ratio:.2f}%\")\n",
    "    print(f\"Space Saved: {csv_size - parquet_size_gb:.2f} GB\")\n",
    "    \n",
    "    # Now read from Parquet to test speed\n",
    "    print(\"\\nReading from Parquet format...\")\n",
    "    start_read = time.time()\n",
    "    \n",
    "    df_parquet = dd.read_parquet(parquet_path)\n",
    "    total_rows = len(df_parquet)\n",
    "    \n",
    "    end_read = time.time()\n",
    "    read_time = end_read - start_read\n",
    "    \n",
    "    print(f\"Parquet read time: {read_time:.2f} seconds\")\n",
    "    print(f\"Total rows: {total_rows:,}\")\n",
    "    \n",
    "    return {\n",
    "        'csv_size_gb': csv_size,\n",
    "        'parquet_size_gb': parquet_size_gb,\n",
    "        'compression_ratio': compression_ratio,\n",
    "        'parquet_read_time': read_time,\n",
    "        'parquet_path': parquet_path\n",
    "    }\n",
    "\n",
    "\n",
    "# Execute Method 3\n",
    "result_method3 = measure_performance(method3_compression, \"Method 3: Compression (Parquet)\")\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "ef49e15f-3453-4ffb-a207-cff225b09587",
   "metadata": {},
   "source": [
    "# 8: Performance Comparison Summary"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "0264db94-abb9-4ce4-8314-be4d319d27ca",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "================================================================================\n",
      "PERFORMANCE COMPARISON - ALL METHODS\n",
      "================================================================================\n",
      "             Method  Execution_Time_Seconds  Memory_Used_MB  Execution_Time_Minutes  Memory_Used_GB\n",
      "   Pandas Chunksize              256.577848       90.285156                4.276297        0.088169\n",
      "     Dask DataFrame             1012.519057      142.414062               16.875318        0.139076\n",
      "Parquet Compression              233.418469       34.078125                3.890308        0.033279\n",
      "\n",
      "================================================================================\n",
      "Fastest Method: Parquet Compression\n",
      "Most Memory Efficient: Parquet Compression\n",
      "================================================================================\n"
     ]
    }
   ],
   "source": [
    "# Create comparison DataFrame\n",
    "comparison_data = {\n",
    "    'Method': [\n",
    "        'Pandas Chunksize',\n",
    "        'Dask DataFrame',\n",
    "        'Parquet Compression'\n",
    "    ],\n",
    "    'Execution_Time_Seconds': [\n",
    "        result_method1['execution_time'],\n",
    "        result_method2['execution_time'],\n",
    "        result_method3['execution_time']\n",
    "    ],\n",
    "    'Memory_Used_MB': [\n",
    "        result_method1['memory_used'],\n",
    "        result_method2['memory_used'],\n",
    "        result_method3['memory_used']\n",
    "    ]\n",
    "}\n",
    "\n",
    "comparison_df = pd.DataFrame(comparison_data)\n",
    "\n",
    "# Add execution time in minutes\n",
    "comparison_df['Execution_Time_Minutes'] = comparison_df['Execution_Time_Seconds'] / 60\n",
    "\n",
    "# Add memory in GB\n",
    "comparison_df['Memory_Used_GB'] = comparison_df['Memory_Used_MB'] / 1024\n",
    "\n",
    "# Display comparison table\n",
    "print(\"\\n\" + \"=\"*80)\n",
    "print(\"PERFORMANCE COMPARISON - ALL METHODS\")\n",
    "print(\"=\"*80)\n",
    "print(comparison_df.to_string(index=False))\n",
    "\n",
    "# Find best performer\n",
    "fastest_method = comparison_df.loc[comparison_df['Execution_Time_Seconds'].idxmin(), 'Method']\n",
    "most_memory_efficient = comparison_df.loc[comparison_df['Memory_Used_MB'].idxmin(), 'Method']\n",
    "\n",
    "print(f\"\\n{'='*80}\")\n",
    "print(f\"Fastest Method: {fastest_method}\")\n",
    "print(f\"Most Memory Efficient: {most_memory_efficient}\")\n",
    "print(f\"{'='*80}\")\n"
   ]
  },
  {
