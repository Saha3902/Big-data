    "# 3: Helper Functions for Memory and Time Measurement"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "187973fc-8bf0-4806-b799-7df68da7a077",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Helper functions loaded successfully!\n",
      "Current memory usage: 148.82 MB\n"
     ]
    }
   ],
   "source": [
    "def get_memory_usage():\n",
    "    \"\"\"\n",
    "    Returns current memory usage in MB\n",
    "    \"\"\"\n",
    "    process = psutil.Process(os.getpid())\n",
    "    mem_info = process.memory_info()\n",
    "    return mem_info.rss / (1024 ** 2)  # Convert bytes to MB\n",
    "\n",
    "\n",
    "def measure_performance(func, method_name):\n",
    "    \"\"\"\n",
    "    Measures execution time and memory usage for a function\n",
    "    \n",
    "    Args:\n",
    "        func: Function to execute\n",
    "        method_name: Name of the method for display\n",
    "        \n",
    "    Returns:\n",
    "        Dictionary containing performance metrics\n",
    "    \"\"\"\n",
    "    print(f\"\\n{'='*70}\")\n",
    "    print(f\"Testing Method: {method_name}\")\n",
    "    print(f\"{'='*70}\")\n",
    "    \n",
    "    # Force garbage collection before measurement\n",
    "    gc.collect()\n",
    "    \n",
    "    # Measure initial memory\n",
    "    mem_before = get_memory_usage()\n",
    "    print(f\"Memory before: {mem_before:.2f} MB\")\n",
    "    \n",
    "    # Measure execution time\n",
    "    start_time = time.time()\n",
    "    \n",
    "    # Execute the function\n",
    "    result = func()\n",
    "    \n",
    "    end_time = time.time()\n",
    "    \n",
    "    # Measure final memory\n",
    "    mem_after = get_memory_usage()\n",
    "    print(f\"Memory after: {mem_after:.2f} MB\")\n",
    "    \n",
    "    # Calculate metrics\n",
    "    execution_time = end_time - start_time\n",
    "    memory_used = mem_after - mem_before\n",
    "    \n",
    "    print(f\"\\nExecution time: {execution_time:.2f} seconds ({execution_time/60:.2f} minutes)\")\n",
    "    print(f\"Memory used: {memory_used:.2f} MB ({memory_used/1024:.2f} GB)\")\n",
    "    \n",
    "    return {\n",
    "        'method': method_name,\n",
    "        'execution_time': execution_time,\n",
    "        'memory_used': memory_used,\n",
    "        'result': result\n",
    "    }\n",
    "\n",
    "\n",
    "# Test the helper function\n",
    "print(\"Helper functions loaded successfully!\")\n",
    "print(f\"Current memory usage: {get_memory_usage():.2f} MB\")\n"
   ]
  },
