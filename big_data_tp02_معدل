      "0             1  500000  151.209281\n",
      "1             2  500000  150.970798\n",
      "2             3  500000  150.597253\n",
      "3             4  500000  151.500571\n",
      "4             5  500000  151.129091\n",
      "5             6  500000  151.720316\n",
      "6             7  500000  151.287207\n",
      "7             8  500000  151.651609\n",
      "8             9  500000  152.657993\n",
      "9            10  500000  151.316204\n"
     ]
    }
   ],
   "source": [
    "def method1_pandas_chunks():\n",
    "    \"\"\"\n",
    "    Method 1: Read large CSV file using pandas with chunksize parameter\n",
    "    This reads the file in smaller chunks to avoid memory overflow\n",
    "    \"\"\"\n",
    "    print(\"\\nStarting to read file in chunks...\")\n",
    "    \n",
    "    # Define chunk size (number of rows per chunk)\n",
    "    # Adjust this based on your available RAM\n",
    "    chunk_size = 500000  # 500k rows per chunk\n",
    "    \n",
    "    # Initialize variables for aggregation\n",
    "    total_rows = 0\n",
    "    total_chunks = 0\n",
    "    chunk_data = []\n",
    "    \n",
    "    # Create an iterator to read CSV in chunks\n",
    "    chunk_iterator = pd.read_csv(\n",
    "        file_path,\n",
    "        chunksize=chunk_size,\n",
    "        parse_dates=['event_time']  # Parse datetime column\n",
    "    )\n",
    "    \n",
    "    # Process each chunk\n",
    "    for i, chunk in enumerate(chunk_iterator, 1):\n",
    "        total_chunks += 1\n",
    "        total_rows += len(chunk)\n",
    "        \n",
    "        # Store basic statistics from each chunk\n",
    "        chunk_info = {\n",
    "            'chunk_number': i,\n",
    "            'rows': len(chunk),\n",
    "            'memory_mb': chunk.memory_usage(deep=True).sum() / (1024**2)\n",
    "        }\n",
    "        chunk_data.append(chunk_info)\n",
    "        \n",
    "        # Print progress every 5 chunks\n",
    "        if i % 5 == 0:\n",
    "            print(f\"  Processed chunk {i}: {len(chunk):,} rows\")\n",
    "        \n",
    "        # Optional: Perform analysis on each chunk here\n",
    "        # Example: chunk.groupby('event_type').size()\n",
    "    \n",
    "    print(f\"\\nTotal chunks processed: {total_chunks}\")\n",
    "    print(f\"Total rows read: {total_rows:,}\")\n",
    "    \n",
    "    # Create summary DataFrame\n",
    "    summary_df = pd.DataFrame(chunk_data)\n",
    "    \n",
    "    return {\n",
    "        'total_rows': total_rows,\n",
    "        'total_chunks': total_chunks,\n",
    "        'summary': summary_df\n",
    "    }\n",
    "\n",
    "\n",
    "# Execute Method 1\n",
    "result_method1 = measure_performance(method1_pandas_chunks, \"Method 1: Pandas with Chunksize\")\n",
    "\n",
    "# Display summary\n",
    "print(\"\\nChunk Summary:\")\n",
    "print(result_method1['result']['summary'].head(10))\n"
   ]
  },
